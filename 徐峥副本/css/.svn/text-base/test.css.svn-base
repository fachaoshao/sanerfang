.box{position: relative;}
.bg{background: url(../img/Z.jpg) no-repeat; width: 100%; height: 100%; background-size: 100% 100%;}
.img_one_yuanhuan{width: 100%; margin-top: 40%;}
.img_one_touxiang{position: absolute; top: 33.7%; left: 26.3%; width: 45%;}

.img_one_ziti{position: absolute; top: 30%; width: 60%; left: 19%;
-webkit-animation: wenzi 8s linear infinite;
-moz-animation: wenzi 8s linear infinite;
-o-animation: wenzi 8s linear infinite;
animation: wenzi 8s linear infinite;
/*transform-origin: 50% 50%;*/
}
@keyframes wenzi{
	from{
		-webkit-transform: rotateZ(0deg);
		-moz-transform: rotateZ(0deg);
		-ms-transform: rotateZ(0deg);
		-o-transform: rotateZ(0deg);
		transform: rotateZ(0deg);
	}
	to{
		-webkit-transform: rotateZ(-360deg);
		-moz-transform: rotateZ(-360deg);
		-ms-transform: rotateZ(-360deg);
		-o-transform: rotateZ(-360deg);
		transform: rotateZ(-360deg);
	}
}

.img_one_sanjiaoxing{position: absolute; top: 19%; left: 10%; width: 9%;}
.img_one_duojiao{position: absolute; left: 84%; width: 8%; top: 29%; transform: rotateZ(300deg);}
